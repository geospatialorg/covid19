{
  "name": "covid",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json --disable-host-check",
    "api": "nodemon api/server.js",
    "build": "ng build && npm run copy-404 && npm run copy-500",
    "build-prod": "ng build --prod --aot && npm run copy-404 && npm run copy-500",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-404": "cpy ./dist/covid/index.html ./dist/covid/ --rename=404.html",
    "copy-500": "cpy ./dist/covid/index.html ./dist/covid/ --rename=500.html",
    "serve-https": "http-server ./dist/covid/ -c-1 --ssl --cert localhost.crt --key localhost.key -p 8080",
    "build-and-serve": "npm run build-prod && npm run serve-https"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.900.7",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@schematics/angular": "^9.0.5",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-piechart-outlabels": "^0.1.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "d3": "^5.15.0",
    "express": "^4.17.1",
    "google-palette": "^1.1.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "ol": "^6.2.1",
    "pg": "^7.18.2",
    "primeflex": "^1.0.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "regression": "^2.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cpy-cli": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^2.0.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "browser": {
    "crypto": false
  }
}
